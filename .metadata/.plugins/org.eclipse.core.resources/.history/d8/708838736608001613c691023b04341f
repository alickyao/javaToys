//线程类
class Run implements Runnable {
	public void run() {
		while (true) {
			try {
				Thread.sleep(1000);
				System.out.println(new Data() {
				});
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				return;
			}
		}
	}
}
